{
    "name": "arifinm/yii2-summernote",
    "description": "Yii2 Summernote widget. Super simple WYSIWYG editor on Bootstrap",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "summernote",
        "widget",
        "wysiwyg"
    ],
    "homepage": "https://github.com/arifinm/yii2-summernote",
    "time": "2022-06-24",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Zelenin",
            "email": "aleksandr@zelenin.me",
            "homepage": "http://zelenin.me",
            "role": "Developer"
        },
        {
            "name": "Joao Marques",
            "email": "joao@jjmf.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/zelenin/yii2-summernote-widget/issues",
        "source": "https://github.com/arifinm/yii2-summernote"
    },
    "require": {
        "yiisoft/yii2": "^2.0.20",
        "bower-asset/codemirror": "@stable",
        "bower-asset/summernote": "^0.8.20"
    },
    "autoload": {
        "psr-4": {
            "arifinm\\summernote\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
